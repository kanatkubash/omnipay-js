{
    "extends": "tslint-config-airbnb",
    "rules": {
        "indent": [
            true,
            "tabs"
        ],
        "ter-indent": [
            true,
            "tab"
        ],
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore"
        ],
        "quotemark": [
            true,
            "single"
        ],
        "no-var-keyword": false,
        "prefer-const": false,
    }
}